    "camelcase": "^6.3.0",
    "chalk": "<5.0.0 >=4.1.2",
    "dts-bundle-generator": "github:bluelovers/dts-bundle-generator#npm-module",
    "enquirer": "^2.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
