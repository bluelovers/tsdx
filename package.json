    "sade": "^1.8.1",
    "safe-variable-name": "^1.0.3",
    "semver": "^7.5.4",
    "shelljs": "^0.8.5",
    "tiny-glob": "^0.2.9",
    "ts-jest": "^29.1.1",
    "ts-type": "^3.0.1",
