    "@types/pretty-quick": "^3.1.0",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-is": "^17.0.3",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/rollup-plugin-json": "^3.0.3",
